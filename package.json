{
  "name": "nodetypescriptapi",
  "version": "1.0.0",
  "description": "Nodejs Api with Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:win": "yarn build && node dist\\src\\index.js",
    "start:ubu": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hicolazu/nodeTypescriptApi.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Henrico Lazuroz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hicolazu/nodeTypescriptApi/issues"
  },
  "homepage": "https://github.com/hicolazu/nodeTypescriptApi#readme",
  "devDependencies": {
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.11.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "jest": "^26.4.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
